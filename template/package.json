{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "android:clean": "cd android && ./gradlew clean",
    "android": "react-native run-android --mode=stagingDebug --appId=com.projectname.staging --active-arch-only",
    "android:prod": "react-native run-android --mode=productionDebug --appId=com.projectname.production --active-arch-only",
    "ios": "react-native run-ios --scheme=projectname-staging",
    "ios:prod": "react-native run-ios --scheme=projectname-production",
    "reverse": "adb reverse tcp:8081 tcp:8081",
    "android:eda": "adb shell setprop debug.firebase.analytics.app com.projectname.staging",
    "android:dda": "adb shell setprop debug.firebase.analytics.app .none.",
    "lint": "eslint src/**/*.{ts,tsx} && tsc --noEmit",
    "test": "jest",
    "postinstall": "patch-package",
    "bundle:analysis": "visualize-bundle -a"
  },
  "dependencies": {
    "@react-native-firebase/app": "^18.5.0",
    "@react-navigation/bottom-tabs": "^6.5.9",
    "@react-navigation/native": "^6.1.8",
    "@react-navigation/native-stack": "^6.9.14",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.5.1",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-native": "0.74.4",
    "react-native-config": "^1.5.1",
    "react-native-mmkv": "^2.10.2",
    "react-native-reanimated": "^3.14.0",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "^13.14.0",
    "react-native-svg-transformer": "^1.2.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.86",
    "@react-native/eslint-config": "0.74.86",
    "@react-native/metro-config": "0.74.86",
    "@react-native/typescript-config": "0.74.86",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4",
    "visualize-bundle": "^1.4.0"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
